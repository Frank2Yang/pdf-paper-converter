{
  "name": "pdf-paper-converter",
  "version": 2,
  "builds": [
    {
      "src": "streamlit_app_vercel.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/streamlit_app_vercel.py"
    }
  ],
  "env": {
    "STREAMLIT_SERVER_HEADLESS": "true",
    "STREAMLIT_BROWSER_GATHER_USAGE_STATS": "false",
    "HF_ENDPOINT": "https://hf-mirror.com"
  }
}
